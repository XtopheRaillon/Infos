(function(a){a.HSCore.components.HSStickyBlock={_baseConfig:{},pageCollection:a(),init:function(b,c){this.collection=b&&a(b).length?a(b):a();if(!a(b).length){return}this.config=c&&a.isPlainObject(c)?a.extend({},this._baseConfig,c):this._baseConfig;this.config.itemSelector=b;this.initStickyBlock();return this.pageCollection},initStickyBlock:function(){var e=this,b=e.config,d=e.pageCollection,c=a(window).width();this.collection.each(function(o,j){var p=a(j),k=Boolean(p.data("responsive")),q=p.attr("class").replace(e.config.itemSelector.substring(1),""),t=p.outerHeight(),l=p.outerWidth(),m=p.parent().width(),s=p.offset().top,n=p.offset().left,f=a.isNumeric(p.data("start-point"))?p.data("start-point"):a(p.data("start-point")).offset().top,r=a.isNumeric(p.data("end-point"))?p.data("end-point"):a(p.data("end-point")).offset().top,h=p.data("has-sticky-header");if(!p.length){return}if(t>(r-f)){return}e.resolutionCheck(p);if(p.hasClass("g-sticky-block--sm")&&c<=576){p.addClass("die-sticky");e.resolutionCheck(p)}else{if(p.hasClass("g-sticky-block--md")&&c<=768){p.addClass("die-sticky");e.resolutionCheck(p)}else{if(p.hasClass("g-sticky-block--lg")&&c<=992){p.addClass("die-sticky");e.resolutionCheck(p)}else{if(p.hasClass("g-sticky-block--xl")&&c<=1200){p.addClass("die-sticky");e.resolutionCheck(p)}else{p.removeClass("die-sticky")}}}}a(window).on("resize",function(){var i=a(window).width();if(p.hasClass("g-sticky-block--sm")&&i<=576){p.addClass("die-sticky");e.resolutionCheck(p)}else{if(p.hasClass("g-sticky-block--md")&&i<=768){p.addClass("die-sticky");e.resolutionCheck(p)}else{if(p.hasClass("g-sticky-block--lg")&&i<=992){p.addClass("die-sticky");e.resolutionCheck(p)}else{if(p.hasClass("g-sticky-block--xl")&&i<=1200){p.addClass("die-sticky");e.resolutionCheck(p)}else{p.removeClass("die-sticky").css({top:"",left:""})}}}}if(k==true){setTimeout(function(){var u=a(this).scrollTop(),v=a("header").outerHeight();t=p.outerHeight(),m=p.parent().width(),s=p.parent().offset().top,n=p.paren