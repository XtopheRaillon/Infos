

Ext.onReady(function()
{var tb_contentedition=new Ext.ux.PfcollToolBar({renderTo:"tb_content"});tb_contentedition.add({xtype:"tbbutton",text:'<i class=\"fa fa-heart-o iconBtnExt\"></i>',tooltip:'Vote',id:'heart',handler:function(){Ext.Ajax.request({url:'https://www.grandpariscirculaire.org/index.php/vote.json',params:{action:'vote',page_id:'871',ressource_type_id:'3',user_id:'',platform:'h'},method:'POST',callback:function(options,success,response){var r=Ext.decode(response.responseText);if(r.message){Ext.MessageBox.show({msg:'<div class="text-center"><div><i class="g-font-size-36 icon-heart g-mb-10"></i></div>'+Ext.util.JSON.decode(response.responseText).message+'</div>',width:300,buttons:Ext.MessageBox.OK,icon:'ext-mb-info'});}
$('#heart_note').html(r.note);}});}});tb_contentedition.add({xtype:"textbox",id:'heart_note',text:''});tb_contentedition.add({xtype:"textbox",text:'<hr>'});});/*share */
